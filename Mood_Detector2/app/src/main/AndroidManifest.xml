        </activity>
        <activity android:name=".DisplayVideo" />
        <activity android:name=".Register" />
        <activity android:name=".Recommendation" />
        <activity android:name=".DeviceListActivity" />
        <activity android:name=".play_song" />
        <activity android:name=".View_Result" />
        <activity android:name=".View_emo"></activity>
    </application>

</manifest>